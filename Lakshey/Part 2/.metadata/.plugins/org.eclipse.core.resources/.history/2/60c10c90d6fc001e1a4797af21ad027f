package student;

public class Student {

	private String name;
	private long phno;
	private int roll;
	private double marks;
	private String college_name="IGU";
	private long adhaarno;
	
	
	public Student(String name, long phno, int roll, double marks) {
		this.name = name;
		this.phno = phno;
		this.roll = roll;
		this.marks = marks;
	}
	public Student(String name, long phno, int roll, double marks, String college_name, long adhaarno) {
		this.name = name;
		this.phno = phno;
		this.roll = roll;
		this.marks = marks;
		this.college_name = college_name;
		this.adhaarno = adhaarno;
	}
	public long getPhno() {
		return phno;
	}
	public void setPhno(long phno) {
		this.phno = phno;
	}
	public double getMarks() {
		return marks;
	}
	public void setMarks(double marks) {
		this.marks = marks;
	}
	public String getName() {
		return name;
	}
	public int getRoll() {
		return roll;
	}
	public String getCollege_name() {
		return college_name;
	}

	public static void main(String[] args) {
		Student s1= new Student("Lucky", 7206169908l, 78, 89.5);
		System.out.println(s1.getCollege_name());
		System.out.println(s1.getMarks());
		System.out.println(s1.getName());
		System.out.println(s1.getPhno());
		System.out.println(s1.getRoll());
	}
}
